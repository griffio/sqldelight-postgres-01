insert:
INSERT INTO Address (address, address2, district, city_id, phone, postal_code) VALUES (?, ?, ?, ?, ?, ?)
RETURNING address_id;

all:
SELECT *
FROM Address;

get:
SELECT *
FROM Address WHERE address_id = ?;

update:
UPDATE Address
SET address = ?, address2 = ?, district = ?, city_id = ?, phone = ?, postal_code = ?
WHERE address_id = ?;
